<!DOCTYPE html>
<html>
<head>

	<title></title>
	<link rel="stylesheet" type="text/css" href="/css/sample.css">
	<script src="/js/jquery-2.2.3.min.js"></script>
	<script src="js/script.js"></script>
</head>
<body>

		<div class="logo">
	
		<% if !current_user %>
			<a href="/"><img src="/img/logo.png"></a>
		<% else %>
			<a href="/user_home"><img src="/img/logo.png"></a>
		<% end %>
		</div>


	<% if flash[:notice] %>
		<div class="flash notice">
		<%=flash[:notice] %>
		</div>
	<% end %>
	
	<% if flash[:alert] %>
		<div class="flash alert">
		<%=flash[:alert] %>
		</div>
	<% end %>
	<br>

	<%=yield %>
</body>
</html>